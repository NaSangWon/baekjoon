#include <stdio.h>

int main() {
	int a, b, c;

	scanf("%d", &a);
	scanf("%d", &b);
	scanf("%d", &c);

	int result = a * b * c;
	int digitCount[10] = { 0 };

	while (result > 0) {
		digitCount[result % 10] += 1;
		result = result / 10;
	}

	for (int i = 0; i < 10; i++)
		printf("%d\n", digitCount[i]);

	return 0;
}
