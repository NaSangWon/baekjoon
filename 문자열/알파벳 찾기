#include <stdio.h>

int main() {
	char input[100] = "";
	int abc[26];

	for (int i = 0; i < 26; i++)
		abc[i] = -1;

	scanf("%s", &input);

	for (int i = 0; i < 100; i++) {
		if (input[i] != 0) {
			int index = (int) input[i] % 97;
			if (abc[index] == -1)
				abc[index] = i;
			else
				continue;
		}
	}
	

	for (int i = 0; i < 26; i++)
		printf("%d ", abc[i]);

	return 0;
}
