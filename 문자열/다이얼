#include <stdio.h>
#include <string.h>

int main() {
	char input[15];
	int len;
	int totalTime = 0;

	scanf("%s", input);
	len = strlen(input);

	for (int i = 0; i < len; i++) {
		int time;
		switch (input[i]) {
		case 'A': case 'B': case 'C':
			time = 3;
			break;
		case 'D': case 'E': case 'F':
			time = 4;
			break;
		case 'G': case 'H': case 'I':
			time = 5;
			break;
		case 'J': case 'K': case 'L':
			time = 6;
			break;
		case 'M': case 'N': case 'O':
			time = 7;
			break;
		case 'P': case 'Q': case 'R': case 'S':
			time = 8;
			break;
		case 'T': case 'U': case 'V':
			time = 9;
			break;
		case 'W': case 'X': case 'Y': case 'Z':
			time = 10;
			break;
		default:
			break;
		}
		totalTime += time;
	}

	printf("%d", totalTime);

	return 0;
}
