#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
	char input[1000001];
	int spaceCount = 0;
	int wordCount = 0;
	int len = 0;

	gets(input);
	len = strlen(input);
	
	for (int i = 0; i < len; i++) {
		if (input[i] == ' ')
			spaceCount++;
	}
	
	wordCount = spaceCount + 1;

	if (len == spaceCount) {
		wordCount = 0;
		printf("%d", wordCount);
	}
	else {
		if (isspace(input[0]))
			wordCount--;
		if (isspace(input[len - 1]))
			wordCount--;
		printf("%d", wordCount);
	}

	return 0;
}
