#include <stdio.h>
#include <string.h>

#define max(x, y) (x) > (y) ? (x) : (y)

void reverse(char *arr, int size, int *newArr) {
	int newIndex = size - 1;
	for (int i = 0; i < size; i++) {
		newArr[newIndex--] = arr[i] - '0';
	}
}

int main() {
	char A[10001], B[10001], result[10002] = {0};
	int AInt[10001] = { 0 }, BInt[10001] = { 0 };
	int lenA = 0, lenB = 0, rIndex = 0;

	scanf("%s %s", A, B);

	lenA = strlen(A);
	lenB = strlen(B);

	int maxLen = max(lenA, lenB);
	

	reverse(A, lenA, AInt);
	reverse(B, lenB, BInt);

	/*for (int i = 0; i < maxLen; i++) {
		printf("%d", AInt[i]);
	}

	printf("\n");

	for (int i = 0; i < maxLen; i++) {
		printf("%d", BInt[i]);
	}

	printf("\n");*/

	for (int i = 0; i < maxLen; i++) {
		int value = AInt[i] + BInt[i] + result[i];
		//printf("value : %d\n", value);
		result[i] = value % 10;
		result[i + 1] += (value / 10);
		//printf("result[%d] : %d\n", i, result[i]);
		//printf("result[%d] : %d\n", i + 1, result[i + 1]);
	}

	//printf("maxLen : %d\n", maxLen);
	/*printf("result : ");

	for (int i = 0; i <= maxLen; i++) {
		printf("%d", result[i]);
	}

	printf("\n");*/

	//printf("result[maxLen] : %d\n", result[maxLen]);
	if (result[maxLen] != 0)
		printf("%d", result[maxLen]);
	for (int i = maxLen - 1; i >= 0; i--) {
		printf("%d", result[i]);
	}

	return 0;
}
