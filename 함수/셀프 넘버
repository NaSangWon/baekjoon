#include <stdio.h>

int selfNumber(int n);

int main() {

	for (int i = 1; i <= 10000; i++) {
		if (selfNumber(i) == 1) {
			printf("%d\n", i);
		}			
	}

	return 0;
}

int selfNumber(int n) {
	for (int i = n; i > 0; i--) {
		int sum = 0;
		int num = i;
		sum = num;
		while (num > 0) {
			sum += num % 10;
			num /= 10;
		}
		if (sum == n)
			return -1;
	}

	return 1;
}
